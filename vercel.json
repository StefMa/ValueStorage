{
  "version": 2,
  "functions": {
    "api/put.go": {
      "includeFiles": "serviceAccount.json"
    },
    "api/get.go": {
      "includeFiles": "serviceAccount.json"
    }
  },
  "cleanUrls": true,
  "rewrites": [
    { "source": "/api/put/:key", "destination": "/api/put" },
    { "source": "/api/get/:key", "destination": "/api/get" }
  ]
}
